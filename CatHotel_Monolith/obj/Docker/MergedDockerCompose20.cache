services:
  cathotel_monolith:
    build:
      context: D:\sibbt\Documents\uni_work\year_3\Semester_2\Final_Year_Project\monolithic_implementation\CatHotel_Monolith
      dockerfile: Dockerfile
    container_name: cathotel_monolith
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: cathotelmonolith
    ports:
    - published: 81
      target: 80
    - target: 80
  ms-sql-server:
    container_name: ms-sql-server
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Express
      SA_PASSWORD: StrongPassword???
    image: mcr.microsoft.com/mssql/server
    ports:
    - target: 1433
  proxy:
    container_name: haproxy
    image: dockercloud/haproxy:alip
    ports:
    - published: 80
      target: 80
    volumes:
    - /var/run/docker.sock:/var/run/docker.sock:rw
version: '3.4'